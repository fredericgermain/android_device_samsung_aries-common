# mount point	fstype		device				options (length for cryptfs encryption footer)

/recovery	mtd		recovery
/boot		mtd		boot
/radio		mtd		radio

/data		ext4		/dev/lvpool/userdata    length=-16384
/cache		yaffs2		cache
/datadata	yaffs2		datadata

/sdcard		vfat		/dev/block/mmcblk0p1    lun=/sys/devices/platform/s3c-usbgadget/gadget/lun0/file
/system		ext4		/dev/lvpool/system

/external_sd		vfat		/dev/block/mmcblk0p3    lun=/sys/devices/platform/s3c-usbgadget/gadget/lun1/file
